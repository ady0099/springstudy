package ex04_DI;

public class MyProcess {
	private String name;
	private String jumin;
	private int gender;
	private Age age;
	
	public MyProcess() {}

	public MyProcess(String name, String jumin, String gender, Age age) {
		this.name = name;
		this.jumin = jumin;
		this.gender = gender;
		this.age = age;
	}
	
	// 실행하고 싶은 메서드
	public void prn() {
		System.out.println("이름 : " + name);
		System.out.println("나이 : " + age.count(jumin, gender));		
	}
	
	
	
	
}